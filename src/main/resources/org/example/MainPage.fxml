<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ChoiceBox?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MainPageController">
   <children>
      <Label layoutX="50.0" layoutY="100.0" text="source" />
      <Label layoutX="50.0" layoutY="200.0" text="dest" />
      <TextField fx:id="sourceAmount" layoutX="350.0" layoutY="100.0" />
      <TextField fx:id="destAmount" layoutX="350.0" layoutY="200.0" />
      <ChoiceBox fx:id="sourceChoiceBox" layoutX="150.0" layoutY="100.0" prefWidth="150.0" />
      <ChoiceBox fx:id="destChoiceBox" layoutX="150.0" layoutY="200.0" prefWidth="150.0" />
      <Button layoutX="200.0" layoutY="300.0" mnemonicParsing="false" onAction="#exchange" text="Exchange" />
   </children>
</AnchorPane>
